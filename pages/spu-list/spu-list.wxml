<view class="container">
    <l-loadmore show type="{{loadingType}}">
      <view slot="content" class="spu-bottom">
        <block wx:for="{{items}}">
          <spu-preview-h data="{{item}}"></spu-preview-h>
        </block>
        <view wx:if="{{empty}}">
          <l-status-show show="{{true}}" type="product" describe="该分类下暂时没有商品~~"></l-status-show>
        </view>
      </view>
    </l-loadmore>
</view>